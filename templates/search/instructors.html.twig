<script>
// Récupérer tous les boutons de navigation
const navButtons = document.querySelectorAll('.nav-button');

// Ajouter un écouteur d'événement à chaque bouton de navigation
navButtons.forEach(button => {
  button.addEventListener('click', () => {
    // Vérifier si le bouton actuel a déjà la classe "active"
    const isActive = button.classList.contains('active');

    // Supprimer la classe "active" de tous les boutons de navigation
    navButtons.forEach(btn => btn.classList.remove('active'));

    // Si le bouton actuel n'est pas déjà actif, ajouter la classe "active" à celui-ci
    if (!isActive) {
      button.classList.add('active');

      // Masquer tous les sous-produits
      navButtons.forEach(btn => {
        const subProducts = btn.querySelector('.sub-products');
        if (subProducts) {
          subProducts.classList.remove('show');
        }
      });

      // Récupérer le sous-produit associé à ce bouton
      const subProducts = button.querySelector('.sub-products');
      
      // Ajouter la classe "show" pour afficher les sous-produits de ce bouton
      if (subProducts) {
        subProducts.classList.add('show');
      }
    } else {
      // Si le bouton actuel est déjà actif, retirer la classe "show" pour fermer les sous-produits
      const subProducts = button.querySelector('.sub-products');
      if (subProducts) {
        subProducts.classList.remove('show');
      }
    }
  });
});


</script>



