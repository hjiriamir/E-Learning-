  border-radius: 99px;
  background-color: grey;
}
#nav-content::-webkit-scrollbar-button {
  height: 15px;
}

#nav-content-highlight {
  position: absolute;
  left: 16px;
  top: -70px;
  width: calc(100% - 16px);
  height: 54px;
  background: white;
  background-attachment: fixed;
  border-radius: 16px 0 0 16px;
  transition: top 0.2s;
