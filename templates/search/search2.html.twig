  right: 0;
  bottom: 100%;
  width: 32px;
  height: 32px;
  border-radius: 50%;
  box-shadow: 16px 16px white;
}
#nav-content-highlight:after {
  top: 100%;
  box-shadow: 16px -16px white;
}
