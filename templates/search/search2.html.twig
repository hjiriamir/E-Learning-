    <script>
      function filterCoachingCards(category) {
    // Get all cards
    var cards = document.getElementsByClassName("cardCoachingProduct");

    // Loop through the cards and show/hide based on the category
    for (var i = 0; i < cards.length; i++) {
        // Get the categories for the current card
        var cardCategories = cards[i].getAttribute("data-category").split(" ");
        
        // Check if the card belongs to the selected category
        if (category === "allCoaching" || cardCategories.includes(category)) {
            cards[i].style.display = "block";
        } else {
            cards[i].style.display = "none";
        }
    }
}
